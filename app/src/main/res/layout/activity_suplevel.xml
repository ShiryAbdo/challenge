<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.challenge.swaqny.challenge.ui.gameplay.Suplevel">

    <LinearLayout
        android:layout_centerInParent="true"
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">



        <com.challenge.swaqny.challenge.ui.components.GameButton
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/element_margin_separation"
            android:text="level one"
            android:id="@+id/levelone" />

        <com.challenge.swaqny.challenge.ui.components.GameButton
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/element_margin_separation"
            android:text="level two"
            android:id="@+id/leveltwo" />

        <com.challenge.swaqny.challenge.ui.components.GameButton
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/element_margin_separation"
            android:text="level three"
            android:id="@+id/levelthree" />

    </LinearLayout>


</android.support.constraint.ConstraintLayout>
